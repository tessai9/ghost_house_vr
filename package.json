{
  "name": "ghost_house",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "webpack --mode=production",
    "serve": "webpack-dev-server",
    "serve:br": "webpack-dev-server --open",
    "serve:vr": "webpack-dev-server --host 0.0.0.0 --https",
    "lint": "eslint -c package.json src"
  },
  "dependencies": {
    "aframe": "^1.0.3",
    "aframe-extras": "^6.1.0",
    "core-js": "^3.4.4",
    "vue": "^2.6.10",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "copy-webpack-plugin": "^5.1.1",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "html-webpack-plugin": "^4.3.0",
    "vue-loader": "^15.9.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "indent": [
        "error",
        2
      ],
      "quotes": [
        "error",
        "double"
      ],
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
